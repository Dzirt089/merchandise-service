{
  "Kestrel": {"EndpointDefaults": {"Protocols": "Http2"}},

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "IncludeScopes": true
    },
    "File": {
      "Path": "D/Logs/app.log",
      "RollingInterval": "Day"
    }
  },
  "AllowedHosts": "*"
}
