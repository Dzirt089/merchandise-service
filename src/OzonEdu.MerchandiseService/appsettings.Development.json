{
    "Logging": {
        "LogLevel": {
            "Default": "Trace",
            "Microsoft.AspNetCore": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        },
        "Console": {
            "IncludeScopes": true
        },
        "File": {
            "Path": "Logs/app.log",
            "RollingInterval": "Day"
        }
    }
    
}
